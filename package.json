{
  "name": "@fastify/busboy",
  "version": "1.0.0",
  "author": "Brian White <mscdex@mscdex.net>",
  "contributors": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com",
      "url": "https://github.com/kibertoad"
    }
  ],
  "description": "A streaming parser for HTML form data for node.js",
  "main": "lib/main",
  "types": "lib/main.d.ts",
  "scripts": {
    "bench:dicer": "node deps/dicer/bench/dicer-bench-multipart-parser.js",
    "lint": "eslint .",
    "lint:everything": "npm run lint && npm run test:types",
    "test:legacy": "node test/test.js",
    "test:mocha": "mocha test",
    "test:types": "tsd",
    "test:coverage": "nyc npm run test",
    "test": "npm run test:mocha && npm run test:legacy"
  },
  "engines": {
    "node": ">=10.17.0"
  },
  "dependencies": {
    "streamsearch": "0.1.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.10",
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "tsd": "^0.19.0",
    "typescript": "^4.5.2"
  },
  "keywords": [
    "uploads",
    "forms",
    "multipart",
    "form-data"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastify/busboy.git"
  },
  "tsd": {
    "directory": "test/types",
    "compilerOptions": {
      "esModuleInterop": false,
      "module": "commonjs",
      "target": "ES2017"
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "lib/*",
    "deps/encoding/*",
    "deps/dicer/lib",
    "deps/dicer/LICENSE"
  ]
}
